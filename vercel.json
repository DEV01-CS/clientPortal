{
  "version": 2,
  "installCommand": "cd client_frontend && chmod +x install.sh && bash install.sh",
  "buildCommand": "cd client_frontend && npm run build",
  "outputDirectory": "client_frontend/build",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

